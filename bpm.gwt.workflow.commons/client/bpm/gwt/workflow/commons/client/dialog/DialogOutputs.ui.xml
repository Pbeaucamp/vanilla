<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with field='labels' type='bpm.gwt.workflow.commons.client.I18N.LabelsCommon' />
		
	<ui:style type="bpm.gwt.workflow.commons.client.dialog.DialogOutputs.MyStyle">
		.contentPanel {
			position: absolute;
			top:50px;
			bottom:0px;
			left:0px;
			right:0px;
		}
		.frame {
		    display: block;
    		border: 0;
    		width: 100%;
    		height: 100%;
		}
		.lstBox {
			width: 100%;
		}
		.container {
			height: 500px;
		    width: 800px;
		    position: relative;
		}
		.containerFull {
			height: 100%;
			width: 100%;
			position: relative;
		}
		.lstBoxContainer {
			position: absolute;
			top:10px;
			bottom:50px;
			left:10px;
			right:10px;
		}
	</ui:style>
	<g:HTMLPanel ui:field="panel" addStyleNames="{style.container}">
		<g:HTMLPanel addStyleNames="{style.lstBoxContainer}">
			<g:ListBox ui:field="lstOutputs" addStyleNames="{style.lstBox}"></g:ListBox>
		</g:HTMLPanel>
		<g:HTMLPanel ui:field="contentPanel" addStyleNames="{style.contentPanel}">
			<g:Frame ui:field="frame" addStyleNames="{style.frame}"></g:Frame>
		</g:HTMLPanel>
	
	</g:HTMLPanel>
</ui:UiBinder> 