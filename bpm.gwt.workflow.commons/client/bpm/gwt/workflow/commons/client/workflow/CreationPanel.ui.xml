<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:bpm.gwt.commons.client.panels">
	
	<ui:style type="bpm.gwt.workflow.commons.client.workflow.CreationPanel.MyStyle">
		.mainPanel {
			width: 100%;
			height: 100%;
		}
		
		.panelNavigation {
			bottom: 10px;
		    position: absolute;
		    top: 10px;
		    width: 295px;
		}
	</ui:style>
	<my:CollapsePanel ui:field="collapsePanel" addStyleNames="{style.mainPanel}"/>
</ui:UiBinder> 