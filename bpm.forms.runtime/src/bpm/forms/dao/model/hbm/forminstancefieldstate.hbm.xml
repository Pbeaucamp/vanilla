<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.forms.model.impl.FormInstanceFieldState" table="FORM_INSTANCE_STATE">
            <id name="id" type="java.lang.Long">
                <column name="FIS_ID" />
                 <generator class="increment"/>
            </id>

			<property name="formInstanceId" type="java.lang.Long">
                <column name="FIS_FI_ID"/>
            </property>
            
           <property name="formFieldMappingId" type="java.lang.Long">
                <column name="FIS_FFM_ID"/>
            </property>           
            
            <property name="value" type="java.lang.String">
                <column name="FIS_VALUE"/>
            </property>
            
            <property name="validated" type="int">
                <column name="FIS_VALIDATED"/>
            </property>
         
    </class>
</hibernate-mapping>