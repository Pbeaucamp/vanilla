<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.forms.model.impl.FormDefinitionTableMapping" table="FORM_TABLE_MAPPING">
            <id name="id" type="java.lang.Long">
                <column name="FTM_ID" />
                 <generator class="increment"/>
            </id>

			<property name="formDefinitionId" type="java.lang.Long">
                <column name="FTM_FD_ID"/>
            </property>
            
            <property name="targetTableId" type="java.lang.Long">
                <column name="FTM_TT_ID"/>
            </property>
            
            
           
			
    </class>
</hibernate-mapping>