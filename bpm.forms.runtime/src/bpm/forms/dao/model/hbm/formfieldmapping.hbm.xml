<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.forms.model.impl.FormFieldMapping" table="FORM_FIELD_MAPPING">
            <id name="id" type="java.lang.Long">
                <column name="FFM_ID" />
                 <generator class="increment"/>
            </id>

			<property name="formDefinitionId" type="java.lang.Long">
                <column name="FFM_FD_ID"/>
            </property>
            
            <property name="targetTableId" type="java.lang.Long">
                <column name="FFM_TT_ID"/>
            </property>
            
            
            <property name="description" type="java.lang.String">
                <column name="FFM_DESCRIPTION"/>
            </property>
            
            <property name="formFieldId" type="java.lang.String">
                <column name="FFM_FORM_FIELD_ID"/>
            </property>
            
            <property name="formFieldName" type="java.lang.String">
                <column name="FFM_FORM_FIELD_NAME"/>
            </property>
            
            <property name="databasePhysicalName" type="java.lang.String">
                <column name="FFM_TABLE_COLUMN_NAME"/>
            </property>
            
            <property name="multivalued" type="java.lang.Integer">
                <column name="FFM_MULTIVALUED"/>
            </property>
            
           <property name="sqlDataType" type="java.lang.String">
                <column name="FFM_SQL_DATA_TYPE"/>
            </property>
			
    </class>
</hibernate-mapping>