<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="bpm.vanilla.repository.beans.datasprovider.ItemsDP" table="rpy_items_datasprovider">
            <id name="id" type="int">
                <column name="id" />
                 <generator class="increment"/>
            </id>
            
            <property name="itemId" type="java.lang.Integer">
                <column name="item_id"/>
            </property>
            
            <property name="datasProviderId" type="java.lang.Integer">
                <column name="data_provider_id"/>
            </property>
            
       	</class>
</hibernate-mapping>