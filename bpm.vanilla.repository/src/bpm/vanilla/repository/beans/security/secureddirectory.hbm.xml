<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.platform.core.repository.SecuredDirectory" table="sec_directory_secured">
            <id name="id" type="java.lang.Integer">
                <column name="id" />
                <generator class="increment"/>
            </id>

            <property name="directoryId" type="java.lang.Integer">
                <column name="directory_id"/>
            </property>


			<property name="groupId" type="java.lang.Integer">
                <column name="group_id"/>
            </property>

                        
            <property name="grant" type="java.lang.String">
                <column name="grant_type"/>
            </property>
            
    </class>
</hibernate-mapping>