<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
	
		.dialog {
			height: 350px;
			width: 500px;
			z-index:20;
		}
		
		.content {
			width: 100%;
			height: 100%;
		}
		
	</ui:style>
	<g:HTMLPanel ui:field="dialog" addStyleNames="{style.dialog}">
		<g:HTMLPanel ui:field="panelContent" addStyleNames="{style.content}"/>
	</g:HTMLPanel>
</ui:UiBinder> 