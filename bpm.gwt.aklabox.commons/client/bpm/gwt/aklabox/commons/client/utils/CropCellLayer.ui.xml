<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="bpm.gwt.aklabox.commons.client.utils.CropCellLayer.MyStyle">
		
		.panel{
		    position: absolute;
		}
		
		.innerPanel{
			position: relative;
			border: 2px solid #59D3EF;
			width: 100%;
			height: 100%;
		}
		.innerPanel:HOVER{
			background-color: #333;
			opacity: 0.5;
		}
		.content{
			text-align: center;
   			width: 100%;
			color: #FFFFFF;
		}
	
	</ui:style>
	<g:HTMLPanel styleName="{style.panel}">
		<g:HTMLPanel ui:field="innerPanel" styleName="{style.innerPanel}">
			 <g:Label ui:field="lblName" styleName="{style.content}"/>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 