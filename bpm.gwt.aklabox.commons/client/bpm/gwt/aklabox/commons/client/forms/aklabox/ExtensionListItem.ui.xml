<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='img' type='bpm.gwt.aklabox.commons.client.images.CommonImages' />
	<ui:with field='i18n' type='bpm.gwt.aklabox.commons.client.I18N.LabelsConstants' />
	<ui:style>
		.lblExt{
			width: 80%;
		}
		.panel{
			padding: 5px;
			border-bottom: 1px solid #e9e9e9;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.panel}">
		<g:Label ui:field="lblExt" addStyleNames="inline {style.lblExt}"/>
		<g:Image ui:field="btnRemove" resource="{img.ic_trash}" title="{i18n.Delete}" addStyleNames="inline"/>
	</g:HTMLPanel>
</ui:UiBinder> 