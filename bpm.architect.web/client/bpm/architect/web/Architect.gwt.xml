<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.6.1//EN"
  "file:///C:/BPM/SDK/gwt-2.6.1/gwt-module.dtd">
<module rename-to='architect'>
	<inherits name='com.google.gwt.user.User'/>
	<inherits name="com.google.gwt.i18n.I18N"/>
	<inherits name="com.google.gwt.core.Core"/>

    <inherits name="bpm.vanilla.platform.core.VanillaPlatform"/>
    <inherits name="bpm.mdm.model.Architect"/>
	<inherits name="bpm.gwt.commons.Commons"/>
	<inherits name="bpm.gwt.workflow.commons.Commons" />
    <inherits name="bpm.update.manager.api.Commons"/>
    <inherits name="bpm.vanilla.map.VanillaMap" />
    
	<inherits name='org.moxieapps.gwt.uploader.Uploader'/>
	<inherits name="gwtupload.GWTUpload"/>
    
	<inherits name='com.google.gwt.user.theme.clean.Clean'/>
	
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>

	<entry-point class='bpm.architect.web.client.Architect'/>

	<!-- Specify the paths for translatable code                    -->
	<source path='client'/>
	<source path='shared'/>
	<public path='pub'/>
	
	<stylesheet src="architect.css"/>

	<!-- allow Super Dev Mode -->
	<add-linker name="xsiframe"/>
</module>
