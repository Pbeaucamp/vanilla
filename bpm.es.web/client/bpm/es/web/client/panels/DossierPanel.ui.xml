<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with field='resources' type='bpm.es.web.client.images.Images' />
	<ui:with field='labels' type='bpm.es.web.client.I18N.Labels' />
	
	<ui:style>
		.mainPanel {
			width: 100%;
			height: 100%;
		}
	</ui:style>
	<g:SimplePanel ui:field="panelContent" addStyleNames="{style.mainPanel}"/>
</ui:UiBinder> 