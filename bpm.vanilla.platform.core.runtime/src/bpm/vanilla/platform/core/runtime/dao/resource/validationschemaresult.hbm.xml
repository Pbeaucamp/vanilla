<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="bpm.vanilla.platform.core.beans.resources.ValidationSchemaResult" table="validation_schema_result">
		<id name="id" type="int">
			<column name="id" />
			<generator class="increment" />
		</id>
		
		<property name="validationId" type="java.lang.Integer">
			<column name="validation_id"/>
		</property>
		
		<property name="validationDate" type="java.util.Date">
			<column name="validation_date"/>
		</property>
		
		<property name="schema" type="string">
			<column name="schema_name"/>
		</property>
		
		<property name="nbLinesCheck" type="java.lang.Integer">
			<column name="nbLines_check"/>
		</property>
		
		<property name="nbLinesError" type="java.lang.Integer">
			<column name="nbLines_error"/>
		</property>
	</class>
</hibernate-mapping>

