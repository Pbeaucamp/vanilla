<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.platform.core.beans.ged.DocCat" table="ged_doc_cat">
            <id name="id" type="java.lang.Integer">
                <column name="DC_ID" />
                 <generator class="increment"/>
            </id>

			<property name="documentId" type="java.lang.Integer">
                <column name="DC_DOC_ID"/>
            </property>
            
			<property name="categoryId" type="java.lang.Integer">
                <column name="DC_CAT_ID"/>
            </property>
    </class>
</hibernate-mapping>