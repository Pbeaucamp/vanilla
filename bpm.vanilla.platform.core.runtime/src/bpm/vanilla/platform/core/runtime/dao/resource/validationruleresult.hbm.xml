<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="bpm.vanilla.platform.core.beans.resources.ValidationRuleResult" table="validation_rule_result">
		<id name="id" type="int">
			<column name="id" />
			<generator class="increment" />
		</id>

		<property name="validationSchemaId" type="java.lang.Integer">
			<column name="validation_schema_id"/>
		</property>
		
		<property name="field" type="string">
			<column name="field"/>
		</property>
		
		<property name="rule" type="string">
			<column name="rule"/>
		</property>
		
		<property name="value" type="string">
			<column name="value" length="10000000"/>
		</property>
	</class>
</hibernate-mapping>

