<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="bpm.vanilla.platform.core.beans.UOlapPreloadBean" table="uolap_query_preload">
            
            <id name="id" type="long">
                <column name="UPL_ID" />
                 <generator class="increment"/>
            </id>

            <property name="repositoryId" type="int">
                <column name="UPL_REPOSITORY_ID"/>
            </property>

            <property name="directoryItemId" type="int">
                <column name="UPL_DIRECTORY_ITEM_ID"/>
            </property>
              <property name="vanillaGroupId" type="int">
                <column name="UPL_GROUP_ID"/>
            </property>
               <property name="mdxQuery" type="string">
                <column name="UPL_MDX" length="60000"/>
            </property>
            
    </class>
</hibernate-mapping>