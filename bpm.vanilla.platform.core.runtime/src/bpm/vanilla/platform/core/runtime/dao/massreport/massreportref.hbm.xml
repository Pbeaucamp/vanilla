<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="bpm.vanilla.platform.core.runtime.dao.massreport.MassReportReference" table="mass_report_references">
            
            <id name="id" type="int">
                <column name="MRRF_ID" />
                 <generator class="increment"/>
            </id>

            <property name="repositoryId" type="int">
                <column name="MRRF_REPOSITORY_ID"/>
            </property>

            <property name="workflowItemId" type="int">
                <column name="MRRF_WORKFLOW_ID"/>
            </property>
            
                        
            <property name="processInstanceId" type="string">
                <column name="MRRF_PROCESS_INSTANCE_UUID"/>
            </property>
            
       </class>
</hibernate-mapping>