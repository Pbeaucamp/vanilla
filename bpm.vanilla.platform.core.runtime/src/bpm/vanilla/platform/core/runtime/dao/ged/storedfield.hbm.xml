<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.platform.core.beans.ged.StoredField" table="ged_stored_field">
            <id name="id" type="java.lang.Integer">
                <column name="STO_ID" />
                 <generator class="increment"/>
            </id>

			<property name="documentId" type="java.lang.Integer">
                <column name="STO_DOC_ID"/>
            </property>
            
            <property name="definitionId" type="java.lang.Integer">
                <column name="STO_FIELD_ID"/>
            </property>
            
            <property name="value" type="string">
                <column name="STO_VALUE"/>
            </property>

    </class>
</hibernate-mapping>