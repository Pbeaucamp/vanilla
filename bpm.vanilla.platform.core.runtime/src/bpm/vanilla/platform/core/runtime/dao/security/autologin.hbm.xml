<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.platform.core.beans.AutoLogin" table="auto_login">
		<id name="id" type="java.lang.Integer">
		    <column name="ID" />
		    <generator class="increment"/>
		</id>
		
		<property name="keyAutoLogin" type="string">
		    <column name="KEY_AUTO_LOGIN"/>
		</property>
		
		<property name="userId" type="int">
		    <column name="USER_ID"/>
		</property>
		            
		<property name="creationDate" type="java.util.Date">
		    <column name="CREATION_DATE"/>
		</property>
		            
		<property name="endValidityDate" type="java.util.Date">
		    <column name="END_VALIDITY_DATE"/>
		</property>
    </class>
</hibernate-mapping>