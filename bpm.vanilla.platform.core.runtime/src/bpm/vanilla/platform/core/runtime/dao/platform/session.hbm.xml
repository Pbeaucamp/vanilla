<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.platform.core.beans.Session" table="vanilla_session">
            <id name="id" type="java.lang.Integer">
                <column name="SE_ID" />
                <generator class="increment"/>
            </id>

            <property name="userId" type="java.lang.Integer">
                <column name="SE_USER_ID"/>
            </property>

			<property name="repositoryId" type="java.lang.Integer">
                <column name="SE_REPOSITORY_ID"/>
            </property>

			<property name="ticket" type="string">
                <column name="SE_TICKET"/>
            </property>

                        
            <property name="beginDate" type="timestamp">
                <column name="SE_BEGIN_DATE"/>
            </property>
            
            <property name="group" type="string">
                <column name="SE_GROUP"/>
            </property>
            
            <property name="refreshDate" type="timestamp">
                <column name="SE_REFRESH_DATE"/>
            </property>
    </class>
</hibernate-mapping>