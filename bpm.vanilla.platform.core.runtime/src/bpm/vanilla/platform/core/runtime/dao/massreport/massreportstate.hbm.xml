<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class name="bpm.vanilla.platform.core.runtime.dao.massreport.MassReportState" table="mass_report_states">
            
            <id name="id" type="long">
                <column name="MRST_ID" />
                 <generator class="increment"/>
            </id>

            <property name="massReportItemId" type="int">
                <column name="MRST_MRRF_ID"/>
            </property>
            
             <property name="reportItemId" type="int">
                <column name="MRST_REPORT_ITEM_ID"/>
            </property>

            <property name="date" type="java.util.Date">
                <column name="MRST_DATE"/>
            </property>

              <property name="activityInstanceUuid" type="string">
                <column name="MRST_ACTIVITY_INSTANCE_UUID"/>
            </property>
             <property name="generated" type="int">
                <column name="MRST_GENERATED"/>
            </property>
            
       </class>
</hibernate-mapping>