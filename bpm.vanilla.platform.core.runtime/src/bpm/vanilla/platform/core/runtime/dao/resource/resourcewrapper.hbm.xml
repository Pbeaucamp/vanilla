<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="bpm.workflow.commons.beans.ResourceWrapper" table="resource_wrapper">
		<id name="id" type="int">
			<column name="id" />
			<generator class="increment" />
		</id>
		
        <property name="type" column="type">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">bpm.vanilla.platform.core.beans.resources.Resource$TypeResource</param>
			</type>
		</property>
        <property name="model" length="10000000"/>
	</class>
</hibernate-mapping>

