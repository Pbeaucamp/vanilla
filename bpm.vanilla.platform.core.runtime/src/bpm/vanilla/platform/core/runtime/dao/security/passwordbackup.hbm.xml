<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.platform.core.beans.PasswordBackup" table="password_backup">
		<id name="id" type="java.lang.Integer">
		    <column name="PB_ID" />
		    <generator class="increment"/>
		</id>
		
		<property name="code" type="string">
		    <column name="PB_CODE"/>
		</property>
		
		<property name="userId" type="int">
		    <column name="PB_USER_ID"/>
		</property>
		            
		<property name="creationDate" type="java.util.Date">
		    <column name="PB_CREATION_DATE"/>
		</property>
		            
		<property name="endValidityDate" type="java.util.Date">
		    <column name="PB_END_VALIDITY_DATE"/>
		</property>
            
        <property name="accepted" type="java.lang.Boolean">
           <column name="PB_IS_ACCEPTED"/>
       </property>
    </class>
</hibernate-mapping>