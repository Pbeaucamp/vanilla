<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.map.model.openlayers.impl.OpenLayersMapSpecificationEntity" table="OPEN_LAYERS_MAP_SPECIFICATION_ENTITIES">
            <id name="id" type="java.lang.Integer">
                <column name="OLMSE_ID" />
                 <generator class="increment"/>
            </id>

            <property name="openLayersMapObjectId" type="java.lang.Integer">
                <column name="OLMSE_MAP_ID"/>
            </property>

            <property name="internalId" type="java.lang.String">
                <column name="OLMSE_INTERNALID"/>
            </property>
            
            <property name="shortName" type="java.lang.String">
                <column name="OLMSE_SHORTNAME"/>
            </property>
            
            <property name="longName" type="java.lang.String">
                <column name="OLMSE_LONGNAME"/>
            </property>
			
    </class>
</hibernate-mapping>