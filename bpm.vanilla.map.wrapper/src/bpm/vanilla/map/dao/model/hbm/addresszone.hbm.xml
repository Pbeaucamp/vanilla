<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.map.model.impl.AddressZone" table="ADDRESS_ZONE">
            <id name="id" type="java.lang.Integer">
                <column name="AZ_ID" />
                 <generator class="increment"/>
            </id>

            <property name="addressId" type="java.lang.Integer">
                <column name="AZ_Address_ID"/>
            </property>

            <property name="mapZoneId" type="long">
                <column name="AZ_Map_Zone_ID"/>
            </property>
			
    </class>
</hibernate-mapping>