<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.map.core.design.opengis.OpenGisCoordinate" table="OPEN_GIS_COORDINATE">
            <id name="id" type="java.lang.Integer">
                <column name="ID" />
                 <generator class="increment"/>
            </id>
            
			<property name="entityId" type="java.lang.Integer">
                <column name="ENTITY_ID"/>
            </property>
            
            <property name="locX" type="java.lang.Double">
                <column name="LOC_X"/>
            </property>
            
            <property name="locY" type="java.lang.Double">
                <column name="LOC_Y"/>
            </property>
            
            <property name="locZ" type="java.lang.Double">
                <column name="LOC_Z"/>
            </property>
			
    </class>
</hibernate-mapping>