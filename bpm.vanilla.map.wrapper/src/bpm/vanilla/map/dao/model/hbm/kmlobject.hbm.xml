<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.map.model.kml.impl.KmlObject" table="KML_OBJECT">
            <id name="id" type="java.lang.Integer">
                <column name="KO_ID" />
                 <generator class="increment"/>
            </id>

            <property name="kmlFileName" type="java.lang.String">
                <column name="KO_File_Name"/>
            </property>
    </class>
</hibernate-mapping>