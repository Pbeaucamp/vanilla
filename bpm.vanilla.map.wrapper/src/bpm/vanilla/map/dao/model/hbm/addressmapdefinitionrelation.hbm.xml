<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.map.model.impl.AddressMapDefinitionRelation" table="ADDRESS_MAP_DEFINITION_RELATION">
            <id name="id" type="java.lang.Integer">
                <column name="AMDR_ID" />
                 <generator class="increment"/>
            </id>

            <property name="addressId" type="java.lang.Integer">
                <column name="AMDR_Address_ID"/>
            </property>

            <property name="mapDefinitionId" type="java.lang.Integer">
                <column name="AMDR_Map_Definition_ID"/>
            </property>
			
    </class>
</hibernate-mapping>