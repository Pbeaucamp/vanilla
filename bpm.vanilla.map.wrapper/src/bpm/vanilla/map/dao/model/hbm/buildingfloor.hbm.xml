<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.map.model.impl.BuildingFloor" table="BUILDING_FLOOR">
            <id name="id" type="java.lang.Integer">
                <column name="BF_ID" />
                 <generator class="increment"/>
            </id>

            <property name="label" type="java.lang.String">
                <column name="BF_Label"/>
            </property>

            <property name="buildingId" type="java.lang.Integer">
                <column name="BF_Building_ID"/>
            </property>

            <property name="level" type="java.lang.Integer">
                <column name="BF_Level"/>
            </property>

            <property name="height" type="java.lang.Integer">
                <column name="BF_Height"/>
            </property>

            <property name="imageId" type="java.lang.Integer">
                <column name="BF_Image_ID"/>
            </property>
			
    </class>
</hibernate-mapping>