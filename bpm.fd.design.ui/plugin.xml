   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="bpm.fd.design.ui.freedashCategory"
            name="FreeDashboard">
      </category>
      <wizard
            category="bpm.fd.design.ui.freedashCategory"
            class="bpm.fd.design.ui.css.wizard.CssImportWizard"
            icon="src/bpm/fd/design/ui/icons/css.png"
            id="bpm.fd.design.ui.css.wizard.CssImportWizard"
            name="%importWizard.Css">
         <description>
            Import a Css file from the local file system into the current project.
         </description>
      </wizard>
      <wizard
            category="bpm.fd.design.ui.freedashCategory"
            class="bpm.fd.design.ui.css.wizard.PictureImportWizard"
            icon="src/bpm/fd/design/ui/icons/picture.png"
            id="bpm.fd.design.ui.css.wizard.PictureImportWizard"
            name="%importWizard.Picture">
         <description>
            Import a Picture file from the local file system into the current project.
         </description>
      </wizard>
      <wizard
            category="bpm.fd.design.ui.freedashCategory"
            class="bpm.fd.design.ui.css.wizard.JavaScriptImportWizard"
            icon="src/bpm/fd/design/ui/icons/javascript.png"
            id="bpm.fd.design.ui.css.wizard.JavaScriptImportWizard"
            name="%importWizard.Js">
         <description>
            Import a JavaScript file from the local file system into the current project.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            class="bpm.fd.design.ui.decorators.Decorator"
            icon="src/bpm/fd/design/ui/icons/error_decoration_16.png"
            id="bpm.fd.design.ui.decorators.Decorator"
            label="label"
            lightweight="true"
            location="BOTTOM_RIGHT"
            state="true">
         <enablement>
            <objectClass
                  name="bpm.fd.api.core.model.IStatuable">
            </objectClass>
         </enablement>
      </decorator>
   </extension>
   <extension
         id="bpm.fd.design.ui.markers.dictionarymarker"
         name="Dictionary"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <attribute
            name="dictionaryname">
      </attribute>
      <attribute
            name="componentname">
      </attribute>
      <attribute
            name="datasetname">
      </attribute>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="bpm.fd.design.ui.freedashCategory"
            name="FreeDashboard">
      </category>
      <wizard
            category="bpm.fd.design.ui.freedashCategory"
            class="bpm.fd.design.ui.css.wizard.CssNewFileWizard"
            descriptionImage="src/bpm/fd/design/ui/icons/css.png"
            icon="src/bpm/fd/design/ui/icons/css.png"
            id="bpm.fd.design.ui.wizard1"
            name="%newWizard.Css">
      </wizard>
      <category
            id="bpm.fd.design.ui.freedashComponentCategory"
            name="Component">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="bpm.fd.design.ui.freedashCategory"
            name="FreeDashboard">
      </category>
      <wizard
            category="bpm.fd.design.ui.freedashCategory"
            class="bpm.fd.design.ui.wizard.ExportFdPackageWizard"
            id="bpm.fd.design.ui.wizard.ExportFdPackageWizard"
            name="Export as Fd package">
         <description>
            Export the project as a freedashboard package
         </description>
      </wizard>
   </extension>
