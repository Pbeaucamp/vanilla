<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="bpm.fd.design.ui.datas.ViewDictionary"
            id="bpm.fd.design.ui.datas.ViewDictionary"
            name="%view.dictionary">
      </view>
      <view
            class="bpm.fd.design.ui.project.views.ProjectView"
            id="bpm.fd.design.ui.project.views.ProjectView"
            name="%view.project">
      </view>
      <view
            class="bpm.fd.design.ui.views.ViewComponent"
            id="bpm.fd.design.ui.views.ViewComponent"
            name="%view.component">
      </view>
      <view
            allowMultiple="false"
            class="bpm.fd.design.ui.editor.part.palette.ViewPalette"
            id="bpm.fd.design.ui.editor.part.palette.ViewPalette"
            name="Design Palette"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="bpm.fd.design.ui.perspectives.DesignPerspective"
            id="bpm.fd.design.ui.perspectives.DesignPerspective"
            name="%perspective.design">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="bpm.fd.design.ui.editors.FdEditor"
            contributorClass="bpm.fd.design.ui.editors.FdEditorContributor"
            icon="icons/sample.gif"
            id="bpm.fd.design.ui.editors.MultipleEditor"
            name="name">
      </editor>
   </extension>
   <extension
         id="bpm.fd.design.ui.nature.freedashboard"
         name="FreeDashboard Project"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="bpm.fd.design.ui.nature.FdNature">
         </run>
      </runtime>
   </extension>
