<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.datatools.connectivity.connectionProfile">
      <newWizard
            name="%newwizard.name"
            icon="icons/new_oda_dswiz.ico"
            profile="%oda.data.source.id"
            description="%newwizard.description"
            class="org.eclipse.datatools.connectivity.oda.design.ui.wizards.NewDataSourceWizard"
            id="%oda.data.source.id">
      </newWizard>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            objectClass="org.eclipse.datatools.connectivity.IConnectionProfile"
            name="%profile.propertypage.name"
            class="bpm.metadata.birt.oda.ui.impl.datasource.FmdtDataSourceProperties"
            id="%oda.data.source.id">
         <filter
               name="org.eclipse.datatools.profile.property.id"
               value="%oda.data.source.id">
         </filter>
      </page>
   </extension>
   <extension
         point="org.eclipse.datatools.connectivity.oda.design.ui.dataSource">
      <dataSourceUI
            id="%oda.data.source.id">
         <newDataSourceWizard
               pageClass="bpm.metadata.birt.oda.ui.impl.datasource.FmdtDataSourcePage"
               includesProgressMonitor="true"
               pageTitle="%wizard.data.source.page.title"
               windowTitle="%wizard.window.title">
         </newDataSourceWizard>
      </dataSourceUI>
      <dataSetUI
            supportsOutParameters="false"
            supportsInParameters="true"
            initialPageId="bpm.metadata.birt.oda.runtime.dataSet.page1"
            id="bpm.metadata.birt.oda.runtime.dataSet">
         <dataSetWizard
               class="org.eclipse.datatools.connectivity.oda.design.ui.wizards.DataSetWizard"
               windowTitle="%wizard.data.set.window.title">
         </dataSetWizard>
         <dataSetPage
               displayName="%wizard.data.set.page.title"
               icon="icons/datasetpage.ico"
               wizardPageClass="bpm.metadata.birt.oda.ui.impl.dataset.FmdtDataSetPage"
               path="/"
               id="bpm.metadata.birt.oda.runtime.dataSet.page1">
         </dataSetPage>
      </dataSetUI>
   </extension>
   <extension
         point="org.eclipse.datatools.connectivity.oda.design.ui.dataSource">
      <dataSourceUI
            id="bpm.metadata.birt.oda.runtime.olap">
         <newDataSourceWizard
               includesProgressMonitor="true"
               pageClass="bpm.metadata.birt.oda.ui.impl.datasource.FmdtDataSourcePage"
               pageTitle="UnitedOlap Metadata datasource"
               windowTitle="UnitedOlap Metadata datasource">
         </newDataSourceWizard>
      </dataSourceUI>
      <dataSetUI
            id="bpm.metadata.birt.oda.runtime.olap.dataset"
            initialPageId="bpm.metadata.birt.oda.runtime.dataSetOlap.page1"
            supportsInParameters="true"
            supportsOutParameters="false">
         <dataSetWizard
               class="org.eclipse.datatools.connectivity.oda.design.ui.wizards.DataSetWizard"
               windowTitle="UnitedOlap Metadata dataset">
         </dataSetWizard>
         <dataSetPage
               displayName="UnitedOlap Metadata dataset"
               icon="icons/datasetpage.ico"
               id="bpm.metadata.birt.oda.runtime.dataSetOlap.page1"
               path="/"
               wizardPageClass="bpm.metadata.birt.oda.ui.impl.dataset.olap.FmdtOlapDatasetPage">
         </dataSetPage>
      </dataSetUI>
   </extension>
   <extension
         point="org.eclipse.datatools.connectivity.connectionProfile">
      <newWizard
            class="org.eclipse.datatools.connectivity.oda.design.ui.wizards.NewDataSourceWizard"
            description="%newwizard.description"
            icon="icons/new_oda_dswiz.ico"
            id="bpm.metadata.birt.oda.runtime.olap"
            name="%newwizard.name"
            profile="bpm.metadata.birt.oda.runtime.olap">
      </newWizard>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="bpm.metadata.birt.oda.ui.impl.datasource.FmdtDataSourceProperties"
            id="bpm.metadata.birt.oda.runtime.olap"
            name="%profile.propertypage.name"
            objectClass="org.eclipse.datatools.connectivity.IConnectionProfile">
         <filter
               name="org.eclipse.datatools.profile.property.id"
               value="bpm.metadata.birt.oda.runtime.olap">
         </filter>
      </page>
   </extension>
   <extension
         point="org.eclipse.equinox.preferences.preferences">
      <initializer
            class=" bpm.metadata.birt.oda.ui.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
