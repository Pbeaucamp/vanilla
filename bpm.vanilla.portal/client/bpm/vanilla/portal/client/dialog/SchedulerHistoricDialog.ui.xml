<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="bpm.vanilla.portal.client.dialog.SchedulerHistoricDialog.MyStyle">
		.pager {
			margin: auto;
		}
		
		.imgGrid {
			opacity: 0.4;
		    filter: literal("alpha(opacity=40)");
		    cursor: pointer;
		    padding: 3px;
		    background-position: center !important
		}
		
		.imgGrid:HOVER {
			opacity: 1.0;
		    filter: literal("alpha(opacity=100)");
		    border-radius: 20px;
		}
		
		.imgGrid:ACTIVE {
		    background-color: rgba(0, 0, 0, 0.2) !important;
		}
	</ui:style>
	<g:HTMLPanel ui:field="contentPanel">   
		<g:SimplePanel ui:field="panelContent"/>
	</g:HTMLPanel>
</ui:UiBinder> 