<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:bpm.faweb.client.panels">
	
	<ui:with field='resources' type='bpm.faweb.client.images.FaWebImage' />
	<ui:with field='label' type='bpm.faweb.client.I18N.LabelsConstants' />
	
	<ui:style type="bpm.faweb.client.panels.center.CubeViewerTab.MyStyle">
		.mainPanel {
			width: 100%;
			height: 100%;
		}
	
		.panelContent {
			position: absolute;
			top: 43px;
			bottom: 0px;
			width: 100%;
			overflow: hidden;
		}
		
		.panelContentViewer {
			position: absolute;
			top: 0px;
			bottom: 0px;
			width: 100%;
			overflow: hidden;
		}

		.gridPanel {
			bottom: 0;
			overflow: auto;
			position: absolute;
			width: 100%;
		}

		.gridPanelTop {
			top: 35px;
		}

		.gridPanelTopExpend {
			top: 100px;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
		<my:CubeToolbar ui:field="toolbar"/>
		<g:SimplePanel ui:field="panelContent" addStyleNames="{style.panelContent}"/>
	</g:HTMLPanel>
</ui:UiBinder> 