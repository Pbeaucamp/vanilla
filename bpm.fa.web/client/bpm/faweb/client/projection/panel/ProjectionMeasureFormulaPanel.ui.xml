	<ui:style>
		
		.lblMeasure {
			width: 100px;	
			margin-top:5px;
		}
		
		.lstType {
			width: 160px;
			height: 18px;
			padding: 0px;
			margin-top:5px;
		}
		
		.btnNewType {
			height: 18px;
			width: 300px;
			padding: 0px;
			margin-top:5px;
			margin-left: 25px;
		}
		
		.mainStyle {
			margin-top: 10px;	
		}
		
		.main {
			margin-top: 25px;
			margin-bottom: 25px;	
		}
		
	</ui:style>
	<g:HorizontalPanel ui:field="mainPanel" styleName="{style.main}">
	
		<!-- <g:Label ui:field="lblMeasure" styleName="{style.lblMeasure}"></g:Label>
		<g:ListBox ui:field="lstType"></g:ListBox> 
		<g:ListBox ui:field="lstType" styleName="{style.lstType}"></g:ListBox>-->
		<g:TextBox ui:field="txtNewType" styleName="{style.btnNewType}"></g:TextBox>
		<g:Image ui:field="imgEditFormula"></g:Image>
	
	</g:HorizontalPanel>
</ui:UiBinder> 