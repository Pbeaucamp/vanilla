<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='label' type='bpm.faweb.client.I18N.LabelsConstants' />
	<ui:style>
	
		.mainPanel {
			width: 600px;
			height: 450px;
		}
		
		.lstBox {
			width: 590px;
			margin:5px;
		}
		
		.colorPanel {
			margin-top: 10px;
			width: 100%;
		}
	
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
	
		<g:HTMLPanel>
			<br/><b><g:Label text="{label.ChooseMap}"></g:Label></b>
			<g:ListBox ui:field="lstMap" addStyleNames="{style.lstBox}"></g:ListBox>
		</g:HTMLPanel>
		
		<g:HTMLPanel>
			<br/><b><g:Label text="{label.ChooseDataset}"></g:Label></b>
			<g:ListBox ui:field="lstDataset" addStyleNames="{style.lstBox}"></g:ListBox>
		</g:HTMLPanel>

		<g:HTMLPanel>
			<br/><b><g:Label text="{label.ChooseMeasure}"></g:Label></b>
			<g:ListBox ui:field="lstMeasure" addStyleNames="{style.lstBox}"></g:ListBox>
		</g:HTMLPanel>

		<g:HTMLPanel>
			<br/><b><g:Label text="{label.ChooseDimension}"></g:Label></b>
			<g:ListBox ui:field="lstDimension" addStyleNames="{style.lstBox}"></g:ListBox>
		</g:HTMLPanel>
		
		<g:VerticalPanel ui:field="colorPanel" addStyleNames="{style.colorPanel}"/>
			
	</g:HTMLPanel>
</ui:UiBinder> 