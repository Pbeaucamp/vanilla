<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="bpm.faweb.client.dialog.FinishSaveDialog.MyStyle">
		.mainPanel {
	    	width: 250px;
		}
		
		.imgResult {
			margin-top: 40px;
			margin-left: 80px;
		}
		
		.msgResult {
			margin-top: 5px;
			margin-left: 5px;
		}
	</ui:style>
	<g:HTMLPanel ui:field="contentPanel" addStyleNames="{style.mainPanel}">
	    <g:Image ui:field="imgResult" />
	    <g:HTML ui:field="msgResult" addStyleNames="{style.msgResult}"/>
	</g:HTMLPanel>
</ui:UiBinder> 