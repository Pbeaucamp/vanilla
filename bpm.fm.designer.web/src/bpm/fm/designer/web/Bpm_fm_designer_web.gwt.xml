<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='FMDesignerWeb'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.i18n.I18N"/>
  <inherits name='bpm.gwt.commons.Commons'></inherits>
  <inherits name="bpm.vanilla.platform.core.VanillaPlatform" />

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!--<inherits name='com.google.gwt.user.theme.clean.Clean'/>-->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/> 
  <inherits name='bpm.fm.api.freeMetrics'></inherits>
  <inherits name='bpm.update.manager.api.Commons'></inherits>
  <inherits name='bpm.vanilla.map.VanillaMap'></inherits>
  <inherits name="gwtupload.GWTUpload"/>

  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='bpm.fm.designer.web.client.FMDesignerWeb'/>
	
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
  
  <extend-property name="locale" values="fr"/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
