<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:bpm.gwt.commons.client.custom">
	
	<ui:with field='labels' type='bpm.gwt.commons.client.I18N.LabelsConstants' />
	
	<ui:style>
		.mainPanel {
			width: 750px;
			height: 500px;
		}
		
		.panelMetrics {
			height: 430px;
			width: 100%;
		}
	
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
<!-- 		<c:LabelTextBox ui:field="txtName" placeHolder="{labels.Name}"/> -->
		<g:SimplePanel ui:field="panelMetrics" addStyleNames="{style.panelMetrics}"/>
	</g:HTMLPanel>
</ui:UiBinder> 