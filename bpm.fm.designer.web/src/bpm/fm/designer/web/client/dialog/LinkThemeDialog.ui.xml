<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='label' type='bpm.fm.designer.web.client.Messages' />
	<ui:with field='resources' type='bpm.fm.designer.web.client.images.Images' />
	
	<ui:style  type="bpm.fm.designer.web.client.dialog.LinkThemeDialog.MyStyle">
		.gridPanel {
			position: absolute;
			width: 100%;
			top: 30px;
			bottom: 50px;
		}
		.grid {
			width: 100%;
			height: 100%;
		}
		.contentPanel {
			width: 500px;
			height: 450px;
		}
	</ui:style>
		<g:HTMLPanel ui:field="contentPanel" addStyleNames="{style.contentPanel}">
			<g:HTMLPanel ui:field="gridPanel" addStyleNames="{style.gridPanel}"/>
		</g:HTMLPanel>
</ui:UiBinder> 