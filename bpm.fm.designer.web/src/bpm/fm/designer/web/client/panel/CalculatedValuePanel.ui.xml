<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='label' type='bpm.fm.designer.web.client.Messages' />
	<ui:with field='resources' type='bpm.fm.designer.web.client.images.Images' />
	
	<ui:style>
		.linePanel {
			margin: 10px;	
		}
		.label {
			margin-bottom: 5px;
		}
		
		.txtBox {
			width: 208px;
		}
		
		.lstBox {
			width: 213px;
		}
		.txtArea {
			display: block;
			resize: none;
			width: 100%;
			height: 100px;
		}
	</ui:style>
	<g:HTMLPanel>
	
		<g:HTMLPanel ui:field="gridPanel">
		
		</g:HTMLPanel>
		
		<g:HTMLPanel addStyleNames="{style.linePanel}">
			<g:CheckBox ui:field="checkKaplan" text="{label.kaplan}"></g:CheckBox>
			<g:Label text="{label.kaplanDef}"></g:Label>
			<g:ListBox ui:field="lstKaplan"></g:ListBox>
		</g:HTMLPanel>
		
		<g:HTMLPanel addStyleNames="{style.linePanel}">
			<g:Label addStyleNames="{style.label}" text="{label.calculation}"></g:Label>
			<g:TextArea ui:field="txtCalculation" styleName="{style.txtArea}"></g:TextArea>
		</g:HTMLPanel>
	
	</g:HTMLPanel>
</ui:UiBinder> 