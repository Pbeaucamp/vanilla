<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="bpm.map.viewer.web.client.wizard.ManageMapFilters.MyStyle">
	
		.panelContent {
			height: 500px;
			width: 400px;
			z-index:20;
		}
		
		.panelTop {
			top: 30px;
			position: absolute;
			height: 40px;
			width: 100%;
		}
		
		.panelBottom {
			top: 70px;
			bottom:50px;
			position: absolute;
			width: 100%;
			overflow: auto;
		}
		
		
		
		.cbxAll {
		    position: absolute;
		    width: 180px;
		    left: 0;
		    line-height: 20px;
		    padding: 10px;
		}
		
		.cbxAny {
		    position: absolute;
		    width: 180px;
		    left: 180px;
		    line-height: 20px;
		    padding: 10px;
		}
		.tree{
			
		}

	</ui:style>
	<g:HTMLPanel addStyleNames="{style.panelContent}">
		<g:HTMLPanel addStyleNames="{style.panelTop}">
			<g:CheckBox ui:field="cbxAll" text="Tout selectionner" addStyleNames="{style.cbxAll}"/>
			<g:CheckBox ui:field="cbxAny" text="Tout dÃ©selectionner" addStyleNames="{style.cbxAny}"/>
		</g:HTMLPanel>
		<g:HTMLPanel addStyleNames="{style.panelBottom}">
			<g:Tree ui:field="treeFilters" addStyleNames="{style.tree}"/>
		</g:HTMLPanel>
		
		
	</g:HTMLPanel>
</ui:UiBinder> 