<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with field='label' type='bpm.map.viewer.web.client.I18N.LabelsConstants' />
	<ui:with field='resources' type='bpm.map.viewer.web.client.images.Images' />
	<ui:with field='commons' type='bpm.gwt.commons.client.images.CommonImages' />
	
	<ui:style>
	
		.mainPanel {
			width: 100%;
			height: 100%;
		}
		
		.info {
			position: absolute;
	        height: 1px;
	        width: 1px;
	        z-index: 100;
		}
	
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
		<g:HTMLPanel ui:field="mapContainer"  addStyleNames="{style.mainPanel}">
			<g:HTMLPanel ui:field="mapInfo" addStyleNames="{style.info}"/>
		</g:HTMLPanel>
		<!-- <div id="mapContainer" style="width: 100%; height: 100%;"></div> -->
	</g:HTMLPanel>
</ui:UiBinder> 