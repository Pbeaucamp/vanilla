    <dataType>0</dataType>
    <scope>0</scope>
  </variable>
  <variable>
    <name>BIGATEWAY_HOME</name>
    <value>/C:/developpement/export/BiGateway/BiGateway//</value>
    <dataType>0</dataType>
    <scope>0</scope>
  </variable>
  <variable>
    <name>GATEWAY_TEMP</name>
    <value>/C:/developpement/export/BiGateway/BiGateway//temp/</value>
    <dataType>0</dataType>
    <scope>0</scope>
  </variable>
</sharedResources>
