            restorable="true">
      </view>
      <view
            class="bpm.vanilla.server.client.ui.clustering.menu.uolap.views.ViewTransfertCache"
            id="bpm.vanilla.server.client.ui.clustering.menu.uolap.views.ViewTransfertCache"
            name="CacheTransfert"
            restorable="true">
      </view>
      <view
            class="bpm.vanilla.server.client.ui.clustering.menu.uolap.views.ManagementView"
            id="bpm.vanilla.server.client.ui.clustering.menu.uolap.views.ManagementView"
            name="Query Management View"
            restorable="true">
      </view>
      <view
            class="bpm.vanilla.server.client.ui.clustering.menu.views.fmdt.FmdtLogsView"
            id="bpm.vanilla.server.client.ui.clustering.menu.views.fmdt.FmdtLogsView"
            name="Fmdt Logged Queries "
            restorable="true">
      </view>
      <view
