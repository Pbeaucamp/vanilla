            name="Uolap Cache">
      </perspective>
      <perspective
            class="bpm.vanilla.server.client.ui.clustering.menu.stress.ProfilingPerspective"
            icon="src/bpm/vanilla/server/client/ui/clustering/menu/icons/clanbomber.png"
            id="bpm.vanilla.server.client.ui.clustering.menu.stress.ProfilingPerspective"
            name="Cluster Profiling">
      </perspective>
