            id="bpm.vanilla.server.client.ui.clustering.menu.uolap.perspectives.CacheTransfertPerspective"
            name="Cache Transfert">
      </perspective>
      <perspective
            class="bpm.vanilla.server.client.ui.clustering.menu.uolap.perspectives.ManagementPerspective"
            icon="src/bpm/vanilla/server/client/ui/clustering/menu/icons/queryMdxManagement.png"
            id="bpm.vanilla.server.client.ui.clustering.menu.uolap.perspectives.ManagementPerspective"
            name="Mdx Query Management">
