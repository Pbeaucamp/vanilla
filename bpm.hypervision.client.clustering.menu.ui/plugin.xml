            id="bpm.vanilla.server.client.ui.clustering.menu.stress.StressView"
            name="Cluster Profiling"
            restorable="true">
      </view>
      <view
            class="bpm.vanilla.server.client.ui.clustering.menu.purgehisto.ViewReportHistoric"
            id="bpm.vanilla.server.client.ui.clustering.menu.purgehisto.ViewReportHistoric"
            name="ReportHistoric"
            restorable="true">
      </view>
      <view
            class="bpm.vanilla.server.client.ui.clustering.menu.views.massreport.MassReportView"
            id="bpm.vanilla.server.client.ui.clustering.menu.views.massreport.MassReportView"
            name="Mass Reporting"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="bpm.vanilla.server.client.ui.clustering.menu.perspectives.PerspectiveMenu"
            icon="src/bpm/vanilla/server/client/ui/clustering/menu/icons/house.png"
            id="bpm.vanilla.server.client.ui.clustering.menu.perspectives.PerspectiveMenu"
            name="Welcome">
      </perspective>
      <perspective
            class="bpm.vanilla.server.client.ui.clustering.menu.uolap.perspectives.CacheTransfertPerspective"
