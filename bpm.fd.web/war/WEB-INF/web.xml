<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
         
	<servlet>
		<servlet-name>loginService</servlet-name>
		<servlet-class>bpm.gwt.commons.server.LoginServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginService</servlet-name>
		<url-pattern>/Dashboard/loginService</url-pattern>
	</servlet-mapping>
	
    <servlet>    
		<servlet-name>commonService</servlet-name>    
		<servlet-class>bpm.gwt.commons.server.CommonServiceImpl</servlet-class>  
	</servlet>    
	<servlet-mapping>    
		<servlet-name>commonService</servlet-name>    
		<url-pattern>/Dashboard/commonService</url-pattern>  
	</servlet-mapping>
	
	<servlet>  
		<servlet-name>cssHelper</servlet-name>  
		<servlet-class>bpm.gwt.commons.server.servlets.CssHelperServlet</servlet-class>  
	</servlet>
	<servlet-mapping>  
		<servlet-name>cssHelper</servlet-name>  
		<url-pattern>/cssHelper</url-pattern>  
	</servlet-mapping>
	
	<servlet>
		<servlet-name>dashboardServlet</servlet-name>
		<servlet-class>bpm.fd.web.server.DashboardServiceImpl</servlet-class>
	</servlet>
  
	<servlet-mapping>
	    <servlet-name>dashboardServlet</servlet-name>
	    <url-pattern>/Dashboard/dashboardService</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>uploadServlet</servlet-name>
		<servlet-class>bpm.gwt.commons.server.servlets.UploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>uploadServlet</servlet-name>
		<url-pattern>CommonUploadServlet.gupld</url-pattern>
	</servlet-mapping>
	
	<servlet>    
		<servlet-name>fmdtService</servlet-name>    
		<servlet-class>bpm.gwt.commons.server.FmdtServicesImpl</servlet-class>  
	</servlet>    
	<servlet-mapping>    
		<servlet-name>fmdtService</servlet-name>    
		<url-pattern>/Dashboard/fmdtService</url-pattern>  
	</servlet-mapping>
  
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
