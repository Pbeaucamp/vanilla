<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:bpm.gwt.commons.client.custom">
	
	<ui:with field='labels' type='bpm.fd.web.client.I18N.Labels' />
	
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	
		<my:CustomCheckbox ui:field="chkLimitCommentNumber" text="{labels.LimitCommentNumber}"/>
		<my:LabelTextBox ui:field="txtCommentNumber" placeHolder="{labels.CommentNumber}"/>
		<my:CustomCheckbox ui:field="chkIsValidation" text="{labels.IsValidation}"/>
	</g:HTMLPanel>
</ui:UiBinder> 