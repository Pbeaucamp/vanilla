<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:bpm.gwt.commons.client.custom">
	
	<ui:with field='labels' type='bpm.fd.web.client.I18N.Labels' />
	
	<ui:style>
		.lbl {
			width: 87%;
			display: inline-block;
		}
		
		.btn {
			width: 10%;
		}
	</ui:style>
	<g:HTMLPanel>
		<my:CustomCheckbox ui:field="isInteractive" text="{labels.IsInteractive}"/>
		
		<my:CustomCheckbox ui:field="showDimensions" text="{labels.ShowDimensions}" enabled="false"/>
		<my:CustomCheckbox ui:field="showCubeFunctions" text="{labels.ShowCubeFunctions}" enabled="false"/>

		<my:LabelTextBox ui:field="lblElements" placeHolder="{labels.DimensionMeasureSelection}" enabled="false" addStyleNames="{style.lbl}"/>
		<my:CustomButton ui:field="btnSelect" text="..." addStyleNames="{style.btn}"/>
	</g:HTMLPanel>
</ui:UiBinder> 