<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:bpm.gwt.commons.client.custom">
	
	<ui:with field='labels' type='bpm.fd.web.client.I18N.Labels' />
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	
		<my:ListBoxWithButton ui:field="columnLabel" label="{labels.ColumnLabel}"></my:ListBoxWithButton>
		<my:ListBoxWithButton ui:field="columnValue" label="{labels.ColumnValue}"></my:ListBoxWithButton>
		<my:ListBoxWithButton ui:field="columnOrder" label="{labels.ColumnOrder}"></my:ListBoxWithButton>
	
	</g:HTMLPanel>
</ui:UiBinder> 