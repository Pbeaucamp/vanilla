<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:bpm.gwt.commons.client.custom">
	
	<ui:with field='labels' type='bpm.fd.web.client.I18N.Labels' />
	
	<ui:style>
		.panelDataOptions {
			
		}
	</ui:style>
	<g:HTMLPanel>
		<c:ListBoxWithButton ui:field="lstDatasources" label="{labels.Datasources}" btnText="..."/>
		<c:ListBoxWithButton ui:field="lstDatasets" label="{labels.Datasets}" btnText="..."/>
		
		<g:SimplePanel ui:field="panelDataOptions" addStyleNames="{style.panelDataOptions}"/>
	</g:HTMLPanel>
</ui:UiBinder> 