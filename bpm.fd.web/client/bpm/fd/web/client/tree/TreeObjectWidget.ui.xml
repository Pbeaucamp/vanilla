<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style type="bpm.fd.web.client.tree.TreeObjectWidget.MyStyle">
		.imgObject {
			float: left;
		}
	
		.lblObject {
			margin-left: 20px;
    		padding-top: 2px;
		}
	</ui:style>
	<g:FocusPanel ui:field="dragPanel">
		<g:HTMLPanel>
			<g:Image ui:field="imgObject"/>
			<g:Label ui:field="lblObject" addStyleNames="{style.lblObject}"/>
		</g:HTMLPanel>
	</g:FocusPanel>
</ui:UiBinder> 