<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="bpm.freemetrics.api.organisation.dashOrTheme.DashboardRelation"
		table="fm_dashboards_relations">
		<id name="id" type="java.lang.Integer">
			<column name="DR_ID" />
			<!--
				<generator class="increment"/>
			-->
		</id>

		<property name="parent_Dash_Id" type="java.lang.Integer">
			<column name="DR_PARENT_DASH_ID" />
		</property>

		<property name="child_Dash_Id" type="java.lang.Integer">
			<column name="DR_CHILD_DASH_ID" />
		</property>
	</class>
</hibernate-mapping>