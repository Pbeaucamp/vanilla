<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="bpm.freemetrics.api.organisation.metrics.AssocCompteurIndicateur"
		table="fm_assoc_compt_indic">
		<id name="id" type="java.lang.Integer">
			<column name="ACI_ID" />
				<generator class="increment" />
		</id>

		<property name="compt_ID" type="java.lang.Integer">
			<column name="ACI_COMPT_ID" />
		</property>

		<property name="indic_ID" type="java.lang.Integer">
			<column name="ACI_INDIC_ID" />
		</property>

	</class>
</hibernate-mapping>