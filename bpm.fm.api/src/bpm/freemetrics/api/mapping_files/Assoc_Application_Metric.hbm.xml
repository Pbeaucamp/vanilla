<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="bpm.freemetrics.api.organisation.relations.appl_metric.Assoc_Application_Metric"
		table="gl_association_territoire_metric">
		<id name="id" type="java.lang.Integer">
			<column name="ASSO_ID" />
			<!--
				<generator class="increment"/>
			-->
		</id>

		<property name="name" type="string">
			<column name="ASSO_NAME" />
		</property>

		<property name="app_ID" type="java.lang.Integer">
			<column name="ASSO_APPLICATION_ID" />
		</property>

		<property name="metr_ID" type="java.lang.Integer">
			<column name="ASSO_METRICS_ID" />
		</property>


	</class>
</hibernate-mapping>