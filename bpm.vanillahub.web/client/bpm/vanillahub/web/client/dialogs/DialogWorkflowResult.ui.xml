<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="bpm.vanillahub.web.client.dialogs.DialogWorkflowResult.MyStyle">
		.mainPanel {
		    height: 115px;
	    	width: 410px;
		}
	
		.imgResult {
		    display: inline-block;
	    	margin: 15px;
		}
		
		.lblResult {
		    display: inline-block;
		    margin-top: 30px;
		    vertical-align: top;
		    width: 300px;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
		<g:Image ui:field="imgResult"/>
		<g:Label ui:field="lblResult" addStyleNames="{style.lblResult}"/>
	</g:HTMLPanel>
</ui:UiBinder> 