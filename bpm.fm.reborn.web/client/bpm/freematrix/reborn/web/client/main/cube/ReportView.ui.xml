<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.fullSize {
			height:100%;
			width:100%;
		}
		.leftPanel {
			width: 250px;
			height: 100%;
		}
		.rightPanel {
			position: absolute;
			top: 0;
			bottom: 0;
			left: 250px;
			right: 0;
		}
		.title {
			background-color: #DDDDDD;
		    font-size: 18px;
		    height: 30px;
		    text-align: center;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.fullSize}">
		<g:HTMLPanel addStyleNames="{style.leftPanel}">
			<g:HTMLPanel addStyleNames="{style.title}"><g:Label>Metrics</g:Label></g:HTMLPanel>
			<g:HTMLPanel ui:field="metricPanel">
			
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel addStyleNames="{style.rightPanel}" ui:field="framePanel">
			<!-- <g:Frame ui:field="frameFa" addStyleNames="{style.fullSize}"></g:Frame> -->
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 