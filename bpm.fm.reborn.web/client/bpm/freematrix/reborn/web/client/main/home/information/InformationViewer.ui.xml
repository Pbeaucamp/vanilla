<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.mainPanel {
			height: 100%;
			width: 100%;
			position: relative;
		}
	</ui:style>
	<g:HTMLPanel ui:field="graphPanel" addStyleNames="{style.mainPanel}">
	</g:HTMLPanel>
</ui:UiBinder> 