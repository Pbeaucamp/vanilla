<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panelUser {
			padding: 20px;
		}
	
		.lstUser {
			width: 200px;   
			margin-top: 20px;
		}
		
		.panelLstUser {
			position: absolute;
			top: 60px;
			bottom: 10px;
			left: 10px;
			width: 210px;
		}
		
		.panelInfos {
			bottom: 10px;
		    left: 230px;
		    position: absolute;
		    right: 10px;
		    top: 80px;
			border: 1px solid black;
			border-radius: 15px;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.panelUser}">
		<g:Image ui:field="btnAddUser"/>
		<g:Image ui:field="btnDeleteUser"/>
		<g:SimplePanel addStyleNames="{style.panelLstUser}">
			<g:ListBox ui:field="lstUser" visibleItemCount="20" addStyleNames="{style.lstUser}"/>
		</g:SimplePanel>
		<g:SimplePanel ui:field="panelInfos" addStyleNames="{style.panelInfos}"/>
	</g:HTMLPanel>
</ui:UiBinder> 