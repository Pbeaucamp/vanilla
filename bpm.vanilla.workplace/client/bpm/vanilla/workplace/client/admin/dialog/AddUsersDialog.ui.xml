<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.lstPack {
			margin: 15px;
    		width: 200px;
		}
		
		.btnConfirm {
		    margin-bottom: 5px;
		    margin-left: 80px;
		    margin-right: 10px
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel>
			<g:ListBox ui:field="lstPackage" visibleItemCount="15" addStyleNames="{style.lstPack}"></g:ListBox>
		</g:HTMLPanel>
		<g:HTMLPanel>
			<g:Button ui:field="btnConfirm" addStyleNames="{style.btnConfirm}">Confirm</g:Button>
			<g:Button ui:field="btnCancel">Cancel</g:Button>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 