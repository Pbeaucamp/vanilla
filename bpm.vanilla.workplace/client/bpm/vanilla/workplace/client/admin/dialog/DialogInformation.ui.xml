<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.lblInfos {
			margin: 10px;
		}
	
		.mainPanel {
			width: 200px;
		}
	
		.important {
		    margin-bottom: 10px;
		    margin-left: 50px;
		    width: 100px;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
		<g:Label ui:field="lblInformations" addStyleNames="{style.lblInfos}"/>
		<g:Button addStyleNames="{style.important}" ui:field="button">OK</g:Button>
	</g:HTMLPanel>
</ui:UiBinder> 