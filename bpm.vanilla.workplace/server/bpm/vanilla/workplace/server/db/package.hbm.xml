<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.vanilla.workplace.shared.model.PlaceWebPackage" table="WK_PACKAGE">
            <id name="id" type="int">
                <column name="ID" />
                 <generator class="increment"/>
            </id>

            <property name="name" type="java.lang.String">
                <column name="NAME"/>
            </property>

            <property name="version" type="java.lang.String">
                <column name="VERSION"/>
            </property>

            <property name="creationDate" type="java.util.Date">
                <column name="CREATION_DATE"/>
            </property>

            <property name="type" type="int">
                <column name="TYPE"/>
            </property>

            <property name="projectId" type="int">
                <column name="PROJECT_ID"/>
            </property>

            <property name="vanillaVersion" type="java.lang.String">
                <column name="VANILLA_VERSION"/>
            </property>

            <property name="creatorId" type="int">
                <column name="CREATOR"/>
            </property>

            <property name="path" type="java.lang.String">
                <column name="PATH"/>
            </property>

            <property name="valid" type="boolean">
                <column name="VALIDATION"/>
            </property>

            <property name="certified" type="boolean">
                <column name="CERTIFIED_BY_BPM"/>
            </property>
            
            <property name="documentationUrl" type="java.lang.String">
            	<column name="DOCUMENTATION_URL"/>
            </property>
            
            <property name="siteWebUrl" type="java.lang.String">
            	<column name="SITE_WEB_URL"/>
            </property>
            
            <property name="prerequisUrl" type="java.lang.String">
            	<column name="PRE_REQUIS_URL"/>
            </property>
    </class>
</hibernate-mapping>