   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.fasd.preferences.PreferenceInitializer"/>
   </extension>
   
   <extension
      point = "org.eclipse.ui.preferencePages">
       <page 
       	id="org.fasd.preference.startup"
	    class="org.fasd.preference.page.PageStartup"
        name="%pref_startup"/>
   </extension>
   
   
	
	<extension
  		point="org.eclipse.ui.newWizards">
		<category
		  name="DataSourceWizard"
		  id="creation.new.datasource">
		</category>
		<wizard
		  name="%wizard_datasource"
		  category="creation.new.datasource"
		  class="org.fasd.cubewizard.DataSourceWizard"
		  project="true"
		  id="creation.newDataSourceWizard">
		</wizard>
  <category
        id="definition.aggregate"
        name="Aggregate Wizard"/>
  <wizard
        category="definition.aggregate"
        class="org.fasd.wizard.aggregate.AggWizard"
        id="creation.wizard.aggregate"
        name="%wizard_aggregate"
        project="true"/>
  <category
        id="definition.inline"
        name="Inline Table Wizard"/>
  <wizard
        category="definition.inline"
        class="org.fasd.wizard.inline.InlineWizard"
        id="creation.wizard.inline"
        name="%wizard_inline"
        project="true"/>
	</extension>
 <extension
       id="product"
       point="org.eclipse.core.runtime.products">
    <product
    	  application="bpm.fasd.client.application"
          name="Vanilla Analysis Designer">
       <property
             name="windowImages"
             value="icons/cube16.gif,icons/cube32.gif,icons/cube48.gif,icons/cube64.gif,icons/cube128.gif">
       </property>
       <property
             name="aboutText"
             value="Vanilla Analysis Designer&#x0A;&#x0A;Version 8&#x0A;&#x0A;Product of BPM-Conseil&#x0A;&#x0A;http://www.bpm-conseil.com/">
       </property>
       <property
             name="aboutImage"
             value="icons/vanilla_about.gif">
       </property>
       <property
             name="appName"
             value="Vanilla Analysis Designer">
       </property>
       <property
             name="preferenceCustomization"
             value="plugin_customization.ini">
       </property>
    </product>
 </extension>
 <extension
       point="org.eclipse.ui.decorators">
    <decorator
          class="org.fasd.viewers.MeasureViewDecorator"
          icon="src/org/freeolap/icons/palette_16.png"
          id="bpm.fasd.client.decorator1"
          label="label"
          lightweight="true"
          location="BOTTOM_RIGHT"
          state="true">
       <enablement>
          <objectClass
                name="org.fasd.utils.trees.TreeMes">
          </objectClass>
       </enablement>
    </decorator>
 </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="bpm.fasd.client.splashHandlers.InteractiveSplashHandler"
            id="bpm.fasd.client.splashHandlers.interactive">
      </splashHandler>
      <splashHandler
            class="org.fasd.splashHandlers.LoginSplashHandler"
            id="org.fasd.splashHandlers.interactive">
      </splashHandler>
      <splashHandlerProductBinding
            productId="bpm.freeolap.product"
            splashId="org.fasd.splashHandlers.interactive">
      </splashHandlerProductBinding>
   </extension>
</plugin>
