   </extension>
   <!--<extension
         id="freeolap"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.freeolap.application"
            name="FreeOlap">
         <property
               name="windowImages"
               value="icons/sample.gif,,,,"/>
