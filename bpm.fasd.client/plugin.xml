<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.freeolap.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.freeolap.Perspective"
            icon="icons/sample.gif"
            id="org.freeolap.perspective"
            name="%perspective_schema">
      </perspective>
      <perspective
            class="org.freeolap.XMLAPerspective"
            id="org.fasd.xmlaperspective"
            name="XMLA">
      </perspective>
	  <!--<perspective
            class="org.freeolap.SecurityPerspective"
            icon="icons/lock16.gif"
            id="org.freeolap.securityperspective"
            name="Admin">
      </perspective>
   <perspective
         class="org.freeolap.XMLAPerspective"
         id="org.fasd.xmlaperspective"
         name="XMLA"/>-->
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="%view_datasource"
            allowMultiple="false"
            class="org.fasd.views.SQLView"
            id="org.fasd.views.sqlView">
      </view>
      <view
            name="%view_dimension"
            allowMultiple="false"
            class="org.fasd.views.DimensionView"
            id="org.fasd.views.dimensionView"/>
      <view
            name="%view_cubes"
            allowMultiple="false"
            class="org.fasd.views.CubeView"
            id="org.fasd.views.cubeView"/>
      <view
            name="%view_properties"
            allowMultiple="false"
            class="org.fasd.views.DetailView"
            id="org.fasd.views.detailView"/>         
      <view
            allowMultiple="false"
            class="org.fasd.views.MeasureView"
            id="org.fasd.views.measureView"
            name="%view_measure">
      </view>
      <view
            class="org.fasd.views.XMLAView"
            id="org.fasd.views.xmlaView"
            name="XMLA"
            restorable="true">
      </view>
