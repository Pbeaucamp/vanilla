<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with field='resources' type='bpm.gwt.commons.client.images.CommonImages' />
	
	<ui:style type="bpm.gwt.commons.client.dialog.FeedbackDialog.MyStyle">
		.mainPanel {
			width: 450px;
			height: 100px;
			overflow: auto;
		}
		
		.txtAbout {
			margin: 20px;
			text-align: center;
		}
	</ui:style>
	<g:HTMLPanel ui:field="contentPanel" addStyleNames="{style.mainPanel}">
		<g:HTML ui:field="txtAbout" addStyleNames="{style.txtAbout}"/>
	</g:HTMLPanel>
</ui:UiBinder> 