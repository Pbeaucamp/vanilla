<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:p="urn:import:bpm.gwt.commons.client.custom.v2">
	
	<ui:style type="bpm.gwt.commons.client.dialog.DatacolumnD4CDialog.MyStyle">
		.mainPanel {
			width: 1200px;
			height: 450px;
			position: relative;
		}
		
		.imgCell {
	
		}
		
		.datagridPanel {
		    display: inline-block;
		    height: 100%;
		    vertical-align: top;
		    width: 100%;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
		<p:GridPanel ui:field="grid" addStyleNames="{style.datagridPanel}, relative"/>
	</g:HTMLPanel>
</ui:UiBinder> 