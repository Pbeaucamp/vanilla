<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style type="bpm.gwt.commons.client.dialog.ButtonTab.MyStyle">
		.button {
		    background: #444444;
		    border: 1px solid #555555;
		    color: #FFFFFF;
		    height: 40px;
		    margin: 0;
		    width: 99%;
		    cursor: pointer;
		    position: relative;
		}
		
		.button:HOVER {
		    background: none repeat scroll 0 0 #EAEAEA;
		    border: 1px solid #555555;
		    color: #000000;
		    height: 40px;
		    margin: 0;
		    width: 99%;
		}
		
		.label {
		    margin-left: 5px;
		    margin-right: 25px;
		    margin-top: 3px;
		}
		
		.selected {
		    background: none repeat scroll 0 0 #EAEAEA;
		    border: 1px solid #555555;
		    color: #000000;
		    height: 40px;
		    margin: 0;
		    width: 99%;
		}
	</ui:style>
	<g:HTMLPanel ui:field="button" addStyleNames="{style.button}">
		<g:Label ui:field="label" addStyleNames="{style.label}"/>
	</g:HTMLPanel>
</ui:UiBinder> 