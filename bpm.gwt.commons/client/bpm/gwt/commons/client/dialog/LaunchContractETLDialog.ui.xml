<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:bpm.gwt.commons.client.custom">
	
	<ui:with field='labels' type='bpm.gwt.commons.client.I18N.LabelsConstants' />
	
	<ui:style>
		.lbl {
			margin: 10px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label text="{labels.SelectAGroupToLaunchTheItemAssociatedToThisContract}" addStyleNames="{style.lbl}"/>
		<c:ListBoxWithButton ui:field="lstGroups" label="{labels.SelectGroups}"/>
	</g:HTMLPanel>
</ui:UiBinder> 