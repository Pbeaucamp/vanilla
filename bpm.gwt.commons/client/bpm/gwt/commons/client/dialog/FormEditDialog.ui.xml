<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:bpm.gwt.commons.client.custom">
	<ui:style>
		.grid {
			height: 249px;
			overflow: auto;
			border-top: 1px solid #D3D3D3;
		}
		.mainPanel {
			height: 600px;
			width: 750px;
			overflow: auto;
		}
		.button {
			margin: 5px;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
		<my:CustomButton ui:field="btnAdd" text="Ajouter un filtre" addStyleNames="{style.button}"/>
		<g:HTMLPanel ui:field="filterPanel" addStyleNames="{style.grid}"/>
		<my:CustomButton ui:field="btnExecute" text="Executer" addStyleNames="{style.button}"/>
		<g:SimplePanel ui:field="gridPanel" addStyleNames="{style.grid}"/>
	</g:HTMLPanel>
</ui:UiBinder> 