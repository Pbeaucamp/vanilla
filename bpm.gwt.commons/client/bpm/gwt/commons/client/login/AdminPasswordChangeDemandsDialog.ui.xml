<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:bpm.gwt.commons.client.custom">
	
	<ui:with field='label' type='bpm.gwt.commons.client.I18N.LabelsConstants' />
	
	<ui:style type="bpm.gwt.commons.client.login.AdminPasswordChangeDemandsDialog.MyStyle">
		.mainPanel {
			height: 250px;
			width: 500px;x;
		}
		
		.imgGrid {
			opacity: 0.4;
		    filter: literal("alpha(opacity=40)");
		    cursor: pointer;
		    padding: 3px;
		    background-position: center !important
		}
		
		.imgGrid:HOVER {
			opacity: 1.0;
		    filter: literal("alpha(opacity=100)");
		    border-radius: 20px;
		}
		
		.imgGrid:ACTIVE {
		    background-color: rgba(0, 0, 0, 0.2) !important;
		}
	</ui:style>
	<g:SimplePanel ui:field="panelGrid" addStyleNames="{style.mainPanel}"/>
</ui:UiBinder> 