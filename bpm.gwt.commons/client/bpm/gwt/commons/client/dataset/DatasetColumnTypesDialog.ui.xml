<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.contentPanel {
			width: 700px;
			height: 420px;
		}
		.gridPanel {
			position: absolute;
			width: 100%;
			top: 30px;
			bottom: 45px;
		}
	</ui:style>
	<g:HTMLPanel ui:field="contentPanel" addStyleNames="{style.contentPanel}">
	
		<g:HTMLPanel ui:field="gridPanel" addStyleNames="{style.gridPanel}"/>
	
	</g:HTMLPanel>
</ui:UiBinder> 