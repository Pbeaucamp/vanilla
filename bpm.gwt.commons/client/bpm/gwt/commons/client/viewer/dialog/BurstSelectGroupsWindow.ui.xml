<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.partButton {
			width: 100%;
			height: 30px;
		}
		
		.partButtonHorizontal {
			position: absolute;
			right: 10px;
		}
		
		.btn {
			cursor: pointer;
		    display: block;
		    margin-right: 5px;
		    width: 80px;
		   	margin-top: 5px;
		   	margin-bottom: 5px;
		   	height:25px;
		}
		
		.contentPanel {
			height: 360px;	
			overflow: auto;
		}
	</ui:style>
	<g:HTMLPanel ui:field="contentPanel"/>
</ui:UiBinder> 