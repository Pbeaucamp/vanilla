<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.lbl {
	        padding: 3px;
		}
		
		.txt {
		    margin-left: 2px;
	    	width: 183px;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Label ui:field="lblParam" addStyleNames="{style.lbl}"/>
		<g:TextBox ui:field="txtParam" addStyleNames="{style.txt}"/>
	</g:HTMLPanel>
</ui:UiBinder> 