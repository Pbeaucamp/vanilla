<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="bpm.profiling.database.bean.AnalysisContentBean" table="analysis_content">
            <id name="id" type="int">
                <column name="AC_ID" />
                 <generator class="increment"/>
            </id>

            <property name="analysisId" type="int">
                <column name="AC_ANALYSIS_ID"/>
            </property>

            <property name="tableName" type="string">
                <column name="AC_TABLE_NAME"/>
            </property>

            <property name="columnName" type="string">
                <column name="AC_COLUMN_NAME"/>
            </property>

           
			
    </class>
</hibernate-mapping>