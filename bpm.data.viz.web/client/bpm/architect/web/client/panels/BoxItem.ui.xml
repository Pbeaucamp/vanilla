<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="bpm.architect.web.client.panels.BoxItem.MyStyle">
	.mainPanel {
		min-height: 70px;
		min-width: 70px;
		max-width: 200px;
		border-radius: 15px;
		background: white;
		box-shadow: 1px 1px 7px #555;
		text-align: center;
		position: relative;
	}
	
	.lblTool {
		padding: 5px;
	}
	</ui:style>
	<g:HTMLPanel ui:field="box" addStyleNames="{style.mainPanel}">
		<g:Label ui:field="lblName" addStyleNames="{style.lblTool}" />
	</g:HTMLPanel>
</ui:UiBinder> 