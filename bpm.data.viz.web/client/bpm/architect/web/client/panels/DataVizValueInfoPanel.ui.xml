<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.mainPanel {
			height: 100%;
			width: 100%;
			vertical-align: top;
			margin-top: 30px;
		}
		
		.contentPanel {
			height: 100%;
			width: 50%;
			display: inline-block;
			-moz-box-sizing: border-box;
		    box-sizing: border-box;
		    vertical-align: top;
		}
		
		.linePanel {
			margin: 10px;
			width: 100%;
		}
		.linePanel div {
			display: inline-block;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.mainPanel}">
		<g:HTMLPanel addStyleNames="{style.contentPanel}">
			<g:HTMLPanel addStyleNames="{style.linePanel}">
				<g:Label text="Count : "/>
				<g:Label ui:field="lblCount"/>
			</g:HTMLPanel>
			<g:HTMLPanel addStyleNames="{style.linePanel}">
				<g:Label text="Distinct : "/>
				<g:Label ui:field="lblDistinct"/>
			</g:HTMLPanel>
			<g:HTMLPanel addStyleNames="{style.linePanel}">
				<g:Label text="Min : "/>
				<g:Label ui:field="lblMin"/>
			</g:HTMLPanel>
			<g:HTMLPanel addStyleNames="{style.linePanel}">
				<g:Label text="Max : "/>
				<g:Label ui:field="lblMax"/>
			</g:HTMLPanel>
			<g:HTMLPanel addStyleNames="{style.linePanel}">
				<g:Label text="Average : "/>
				<g:Label ui:field="lblAvg"/>
			</g:HTMLPanel>
		</g:HTMLPanel><g:HTMLPanel addStyleNames="{style.contentPanel}">
			<g:HTMLPanel addStyleNames="{style.linePanel}">
				<g:Label text="Min lenght : "/>
				<g:Label ui:field="lblLenghtMin"/>
			</g:HTMLPanel>
			<g:HTMLPanel addStyleNames="{style.linePanel}">
				<g:Label text="Max lenght : "/>
				<g:Label ui:field="lblLenghtMax"/>
			</g:HTMLPanel>
			<g:HTMLPanel addStyleNames="{style.linePanel}">
				<g:Label text="Avg lenght : "/>
				<g:Label ui:field="lblLenghtAvg"/>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 