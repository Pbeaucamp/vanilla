   <configIni use="default">
      <linux>/metadataclient/config.ini</linux>
      <macosx>/metadataclient/config.ini</macosx>
      <solaris>/metadataclient/config.ini</solaris>
   </configIni>

   <launcherArgs>
      <programArgs>-Xms512M
-Xmx1024M
-clearPersistedState
      </programArgs>
      <vmArgs>-Dlog4j.configuration=log.xml -Dosgi.user.area=@user.home/.bpmconseil/serie8/freemetadata/designer -Dosgi.instance.area.default=@user.home/.bpmconseil/serie8/freemetadata/designer
      </vmArgs>
      <vmArgsLin>-Dbpm.vanilla.configurationFile=./resources/vanilla.properties
      </vmArgsLin>
      <vmArgsMac>-Dbpm.vanilla.configurationFile=../../../resources/vanilla.properties -XstartOnFirstThread -Dorg.eclipse.swt.internal.carbon.smallFonts
      </vmArgsMac>
      <vmArgsWin>-Dbpm.vanilla.configurationFile=./resources/vanilla.properties
      </vmArgsWin>
   </launcherArgs>

   <windowImages i16="icons/fmdt-16.png" i32="icons/fmdt-32.png"/>

   <splash
      location="bpm.metadata.client" />
   <launcher name="VanillaMetadata">
