<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>loginService</servlet-name>
		<servlet-class>bpm.gwt.commons.server.LoginServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginService</servlet-name>
		<url-pattern>/Metadata/loginService</url-pattern>
	</servlet-mapping>
	
    <servlet>    
		<servlet-name>commonService</servlet-name>    
		<servlet-class>bpm.gwt.commons.server.CommonServiceImpl</servlet-class>  
	</servlet>    
	<servlet-mapping>    
		<servlet-name>commonService</servlet-name>    
		<url-pattern>/Metadata/commonService</url-pattern>  
	</servlet-mapping>
	
	<servlet>  
		<servlet-name>cssHelper</servlet-name>  
		<servlet-class>bpm.gwt.commons.server.servlets.CssHelperServlet</servlet-class>  
	</servlet>
	<servlet-mapping>  
		<servlet-name>cssHelper</servlet-name>  
		<url-pattern>/cssHelper</url-pattern>  
	</servlet-mapping>

	<servlet>
		<servlet-name>metadataService</servlet-name>
		<servlet-class>bpm.metadata.web.server.MetadataServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>metadataService</servlet-name>
		<url-pattern>/Metadata/metadataService</url-pattern>
	</servlet-mapping>
	
    <servlet>    
		<servlet-name>fmdtService</servlet-name>    
		<servlet-class>bpm.gwt.commons.server.FmdtServicesImpl</servlet-class>  
	</servlet>    
	<servlet-mapping>    
		<servlet-name>fmdtService</servlet-name>    
		<url-pattern>/Metadata/fmdtService</url-pattern>  
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
