<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.datatools.connectivity.connectionProfile">
      <newWizard
            name="%newwizard.name"
            icon="icons/new_oda_dswiz.ico"
            profile="%oda.data.source.id"
            description="%newwizard.description"
            class="org.eclipse.datatools.connectivity.oda.design.ui.wizards.NewDataSourceWizard"
            id="%oda.data.source.id">
      </newWizard>
   </extension>
   <extension
         point="org.eclipse.datatools.connectivity.ui.connectionProfileImage">
      <profileImage
            icon="icons/profile.gif"
            profileID="%oda.data.source.id">
      </profileImage>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="bpm.inlinedatas.oda.driver.ui.impl.datasource.EditDataSourceInLine"
            id="%oda.data.source.id"
            name="Inline Edit DataSource"
            objectClass="org.eclipse.datatools.connectivity.IConnectionProfile">
         <filter
               name="org.eclipse.datatools.profile.property.id"
               value="%oda.data.source.id">
         </filter>
      </page>
   </extension>
   <extension
         point="org.eclipse.datatools.connectivity.oda.design.ui.dataSource">
      <dataSourceUI
            id="%oda.data.source.id">
         <newDataSourceWizard
               pageClass="bpm.inlinedatas.oda.driver.ui.impl.datasource.BuildDataSourcePage"
               includesProgressMonitor="false"
               pageTitle="%wizard.data.source.page.title"
               windowTitle="%wizard.window.title">
         </newDataSourceWizard>
      </dataSourceUI>
      <dataSetUI
            supportsOutParameters="false"
            supportsInParameters="true"
            initialPageId="bpm.inlinedatas.oda.driver.runtime.dataSet.page1"
            id="bpm.inlinedatas.oda.driver.runtime.dataSet">
         <dataSetWizard
               class="org.eclipse.datatools.connectivity.oda.design.ui.wizards.DataSetWizard"
               windowTitle="%wizard.data.set.window.title">
         </dataSetWizard>
         <dataSetPage
               displayName="%wizard.data.set.page.title"
               id="bpm.inlinedatas.oda.driver.runtime.dataSet.page1"
               path="/"
               wizardPageClass="bpm.inlinedatas.oda.driver.ui.impl.dataset.BuildDataSetPage">
         </dataSetPage>
         <dataSetPage
               id="Filters InLine ODA Driver"
               wizardPageClass="bpm.inlinedatas.oda.driver.ui.impl.dataset.FilterDataSetPage">
         </dataSetPage>
      </dataSetUI>
   </extension>

</plugin>
