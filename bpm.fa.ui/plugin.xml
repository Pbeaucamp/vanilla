<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="bpm.fa.ui.views.CubeExplorationView"
            id="bpm.fa.ui.views.CubeExplorationView"
            name="%view.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="bpm.fa.ui.perspective.Perspective"
            id="bpm.fa.ui.perspective.Perspective"
            name="%perspective.name">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="bpm.fa.ui.commands.category"
            name="%category.name">
      </category>
      <command
            categoryId="bpm.fa.ui.commands.category"
            id="bpm.fa.ui.commands.sampleCommand"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="bpm.fa.ui.handlers.PerspectuveHandler"
            commandId="bpm.fa.ui.commands.sampleCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="bpm.fa.ui.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=tools">
         <menu
               id="bpm.fa.ui.menus.sampleMenu"
               label="%menu.label"
               mnemonic="M">
            <command
                  commandId="bpm.fa.ui.commands.sampleCommand"
                  id="bpm.fa.ui.menus.sampleCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="bpm.fa.ui.toolbars.sampleToolbar">
            <command
                  commandId="bpm.fa.ui.commands.sampleCommand"
                  icon="src/bpm/fa/ui/icons/sample.gif"
                  id="bpm.fa.ui.toolbars.sampleCommand"
                  tooltip="%command.tooltip">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
